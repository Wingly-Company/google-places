{
    "name": "wingly/google-places",
    "description": "Server side Google places api",
    "authors": [
        {
            "name": "Dimitris Karapanos",
            "email": "dimitris@wingly.io",
            "homepage": "https://www.wingly.io",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/wingly/google-places",
    "keywords": [
        "Laravel",
        "Google Places"
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Wingly\\GooglePlaces\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wingly\\GooglePlaces\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wingly\\GooglePlaces\\GooglePlacesServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
